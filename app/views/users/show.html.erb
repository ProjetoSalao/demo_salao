<div class="page-header">
  <%= link_to users_path, class: 'btn btn-default' do %>
    All Users
  <% end %>
  <%= link_to edit_user_registration_path(@user), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show user</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @user.first_name %></dd>
  <dt>Subdomain:</dt>
  <dd><%= @user.last_name %></dd>
  <dt>Domain:</dt>
  <dd><%= @user.email %></dd>
</dl>
